SOL_PREFIX4="192.168.1."
SOL_PREFIX6="2001:db8:ca2:6:6::1:"
SOL_SUBNET4="192.168.1.0/24"
SOL_SUBNET6="2001:db8:ca2:6:6::0/64"
SOL_SUBNETS="${SOL_SUBNET4} ${SOL_SUBNET6}"

# new fsinf shell-server
PLUTO4_LOCAL="${SOL_PREFIX4}23"
